<template slot="contentpage" :title="^title" :hlevel="^h|1" :d-none="=>':title'===''&& 'd-none'" >
  <span class="h:hlevel">:title</span>
  <div style="opacity: 0;" class=":d-none">_</div>
  <nav>
    <div class="nav nav-tabs" role="tablist">
      <slot name="tabs" />
    </div>
  </nav>
  <div style="opacity: 0;">_</div>
  <div class="tab-content">
      <slot name="tabcontent" />
  </div>
</template>
